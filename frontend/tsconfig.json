{
    "compilerOptions": {
        "jsx": "react",
        "experimentalDecorators": true,
        "alwaysStrict": true,
        "noImplicitAny": false,
        "forceConsistentCasingInFileNames": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "strict": true
    },
    "exclude": [
        "*/dist/*"
    ]
}
